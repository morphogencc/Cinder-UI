<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <CINDER_UI_ROOT>..\..\..\..\dependencies\cinder-ui</CINDER_UI_ROOT>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>cinder-ui-production</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(CINDER_UI_ROOT)\src;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="CINDER_UI_ROOT">
      <Value>$(CINDER_UI_ROOT)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>
